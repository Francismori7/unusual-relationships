{
    "name": "shomisha/unusual-relationships",
    "description": "This package offers a trait which provides your models with the ability to have relationships not included within standard Laravel installations.",
    "keywords": [
        "laravel",
        "relationships",
        "eloquent",
        "model",
        "database"
    ],
    "homepage": "https://github.com/shomisha/unusual-relationships",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mi≈°a Kovic",
            "email": "misa95kovic@gmail.com"
        }
    ],
    "require": {
        "laravel/framework": "~5.6",
        "php": "~7.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.8",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Shomisha\\UnusualRelationships\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shomisha\\UnusualRelationships\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --config tests/phpunit.xml --bootstrap vendor/autoload.php"
    }
}
